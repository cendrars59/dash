 <%= search_form_for @search do |f| %>

   <div class="form-group spacing-search">
     <%= f.label :code_or_description_cont, "Entry" %>
   </div>
   <div class="form-group spacing-search">
      <%= f.text_field :code_or_description_cont %>
   </div>
   <div class="form-group spacing-search">
      <%= f.label "Type" %>
   </div>
   <div class="form-group spacing-search">
      <%= f.select options_for_select(@selectable_types) %>
   </div>
   <div class="form-group spacing-search">

   </div>
   <div class="form-group spacing-search">
       <%= f.submit "Search" %>
   </div>

 <% end %>
